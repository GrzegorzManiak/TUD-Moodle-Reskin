{
    "manifest_version": 2,
    "name": "TUD Tallaght Reskin",
    "version": "1.0",
  
    "description": "TUD Tallaght Moodle Reskin",       
  
    "icons": {
      "48": "icons/icon.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://elearning-ta.tudublin.ie/*"],
        "js": ["dist/bundle.js"]
      }
    ],

    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "*://elearning-ta.tudublin.ie/*",
      "*://api.github.com/*",
      "*://github.com/*",
      "*://raw.githubusercontent.com/*",
      "contextMenus"
    ],

    "browser_specific_settings": {
      "gecko": {
        "id": "git@grzegorz.ie"
      }
    }
}
  